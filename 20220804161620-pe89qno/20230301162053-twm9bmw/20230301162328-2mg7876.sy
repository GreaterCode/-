		{
			"ID": "20230301162328-37y8by1",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20230301162328-37y8by1"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "flanel.1设备，就是VXLAN的VTEP，即有IP地址，也有MAC地址"
				}
