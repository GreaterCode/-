			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "可以在安装脚本中进行配置："
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "code",
					"TextMarkTextContent": "ENABLE_LB=false"
				}
			]
		},
		{
			"ID": "20221127101257-ec72h4k",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20221127101257-ec72h4k",
				"updated": "20221127101310"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "或者在安装后通过更改 "
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "code",
					"TextMarkTextContent": "kube-ovn-controller"
				},
				{
					"Type": "NodeText",
					"Data": " Deployment 的参数进行配置："
				}
			]
		},
