			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "1.1 核心能力"
				}
			]
		},
		{
			"ID": "20230516025810-nsrk1fq",
			"Type": "NodeList",
			"ListData": {},
			"Properties": {
