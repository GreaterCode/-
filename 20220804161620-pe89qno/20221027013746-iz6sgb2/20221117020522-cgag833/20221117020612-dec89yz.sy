		},
		{
			"ID": "20221117021651-kmendek",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20221117021651-kmendek",
				"updated": "20221117021717"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "\t下载“ "
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "strong",
					"TextMarkTextContent": "fcct-x86_64-unknown-linux-gnu"
				},
				{
					"Type": "NodeText",
					"Data": " ”文件，另存为fcct，该文件是用来将基于YAML格式的配置文件转换成基于JSON格式的Ignition文件。"
				}
			]
		},
