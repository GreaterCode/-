		{
			"ID": "20221112170645-uxqkrl3",
			"Type": "NodeParagraph",
			"Properties": {
				"id": "20221112170645-uxqkrl3",
				"updated": "20221112170801"
			},
			"Children": [
				{
					"Type": "NodeText",
					"Data": "\t要安装 Operator，集群管理员或被授予 Operator 安装权限的用户，必须"
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "mark",
					"TextMarkTextContent": "首先创建一个 Subscription 对象"
				},
				{
					"Type": "NodeText",
					"Data": "。随后 OpenShift 的 OLM 会根据 Subscription "
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "mark",
					"TextMarkTextContent": "创建一个 InstallPlan 对象来安装 Operator 资源"
				},
				{
					"Type": "NodeText",
					"Data": "。InstallPlan 被 Subscription 用来执行"
				},
				{
					"Type": "NodeTextMark",
					"TextMarkType": "mark",
					"TextMarkTextContent": "安装 CVS"
				},
				{
					"Type": "NodeText",
					"Data": "，并记录安装结果。"
				}
			]
		},
