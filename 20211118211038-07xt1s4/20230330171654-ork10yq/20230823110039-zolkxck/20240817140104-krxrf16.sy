						"id": "20240817140233-x3e67bq"
					},
					"Children": [
						{
							"ID": "20240817140233-74fzk7g",
							"Type": "NodeParagraph",
							"Properties": {
								"id": "20240817140233-74fzk7g",
								"updated": "20240817140235"
							},
							"Children": [
								{
									"Type": "NodeTextMark",
									"TextMarkType": "em",
									"TextMarkTextContent": "–compress=lz4产生"
								},
								{
									"Type": "NodeText",
									"Data": ".lz4文件。您可以使用诸如的程序来提取这些文件的内容lz4。"
								}
							]
						}
					]
				},
				{
					"ID": "20240817140236-2rsguej",
					"Type": "NodeListItem",
					"ListData": {
						"BulletChar": 42,
						"Marker": "Kg=="
					},
					"Properties": {
